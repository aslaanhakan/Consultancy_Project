@model HomePageViewModel
@{
    ViewData["Title"] = "Home Page";
}
<!-- Carousel Start -->
<div class="container-fluid p-0 mt-3">
    <div class="position-relative">
        <div class=" position-relative">
            <img class="img-fluid" src="~/Images/img/carousel-1.jpg" alt="">
            <div class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center" style="background: rgba(43, 57, 64, .5);">
                <div class="container">
                    <div class="row justify-content-center">
                        <div class="col-10 col-lg-8  text-center">
                            <h2 class="display-4 text-white animated  mb-4">Zihninize iyi gelecek bir adım:</h2>
                            <h3 class="display-6 text-white animated mb-4"> Online terapiye başlayın</h3>
                            <p class="fs-5 fw-medium text-white mb-4 pb-2">Zihninizi dinlendirmek, kendinize zaman ayırmak için mükemmel bir fırsat: <strong>Consultancy!</strong> Hemen kaydolun ve hayatınıza biraz nefes aldırın.</p>
                            @if (!User.Identity.IsAuthenticated)
                            {
                                <a asp-area="" asp-controller="Account" asp-action="Register" class="btn btn-primary py-md-3 px-md-5 me-3 animated slideInLeft">Kayıt Ol</a>
                            }
                            <a href="" class="btn btn-secondary py-md-3 px-md-5 animated slideInRight">Uzmanları Gör</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
<!-- Carousel End -->
