                                        <h5 class="mb-3">@consultant.User.FirstName @consultant.User.LastName</h5>
                                        <h6 class="mb-3 ">@consultant.JobTitle</h6>
                                        <div class=" mb-3">
                                            <span class=" me-3"><i class="far fa-clock text-primary me-2"></i>Görüntülü 45 dk./ Sesli Görüşme 45 dk.</span> 
                                            <span class=" me-0"><i class="far fa-money-bill-alt text-primary me-2"></i>@($"{consultant.VisitsPrice:C2}")</span>
                                        </div>
                                        <div class="mb-3">
                                            @foreach (var item in consultant.ConsultantsSpecializations)
